  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jest": "^29.2.0",
    "colors": "^1.4.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.25.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^25.3.2",
    "jest": "^29.2.1",
    "lint-staged": "^13.0.3",
